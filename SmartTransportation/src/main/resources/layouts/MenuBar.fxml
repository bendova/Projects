<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>

<Pane fx:id="container" prefHeight="34.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="gui.timeline.TimeLineController">
  <children>
    <Rectangle fx:id="background" arcHeight="5.0" arcWidth="5.0" fill="#bfbfbf" height="36.0" layoutX="0.0" layoutY="0.0" stroke="BLACK" strokeType="INSIDE" width="600.0">
      <effect>
        <Lighting>
          <bumpInput>
            <Shadow />
          </bumpInput>
          <light>
            <javafx.scene.effect.Light.Distant color="#fdfdfd" />
          </light>
        </Lighting>
      </effect>
    </Rectangle>
    <VBox id="VBox" alignment="CENTER" layoutX="0.0" layoutY="-1.0" prefHeight="36.0" spacing="5.0">
      <children>
        <HBox fx:id="controlsContainer" maxHeight="-1.0" maxWidth="-1.0" prefHeight="32.0" prefWidth="600.0" spacing="10.0">
          <children>
            <ToggleButton fx:id="toggle" mnemonicParsing="false" text="Play/Pause" HBox.hgrow="NEVER" />
            <Slider fx:id="timeLineSlider" onDragDetected="#startSliderDrag" prefWidth="480.0" value="0.0">
              <HBox.margin>
                <Insets top="4.0" />
              </HBox.margin>
            </Slider>
          </children>
          <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
          </padding>
        </HBox>
      </children>
      <padding>
        <Insets top="2.0" />
      </padding>
    </VBox>
  </children>
</Pane>
